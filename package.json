{
  "name": "web-csv-toolbox",
  "version": "0.5.2",
  "description": "A CSV Toolbox utilizing Web Standard APIs.",
  "type": "module",
  "main": "dist/cjs/web-csv-toolbox.cjs",
  "module": "dist/es/web-csv-toolbox.js",
  "types": "dist/types/web-csv-toolbox.d.ts",
  "unpkg": "dist/web-csv-toolbox.umd.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/es/web-csv-toolbox.js",
      "require": "./dist/cjs/web-csv-toolbox.cjs",
      "types": "./dist/types/web-csv-toolbox.d.ts",
      "default": "./dist/es/web-csv-toolbox.js"
    },
    "./**/*": {
      "import": "./dist/es/*.js",
      "require": "./dist/cjs/*.cjs",
      "types": "./dist/types/*.d.ts",
      "default": "./dist/es/*.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "doc": "typedoc",
    "test": "vitest",
    "test:browser": "vitest --browser",
    "build": "npm run build:js && npm run build:browser",
    "build:js": "vite build",
    "build:browser": "vite build --config config/vite.config.umd.ts",
    "serve": "vite serve",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamiazya/web-csv-toolbox.git"
  },
  "keywords": [
    "csv",
    "parser",
    "web-streams",
    "web-streams-api"
  ],
  "author": "Yuki Yamazaki <yuki@kamiazya.tech>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kamiazya/web-csv-toolbox/issues"
  },
  "homepage": "https://kamiazya.github.io/web-csv-toolbox/",
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@fast-check/vitest": "^0.0.9",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitest/browser": "^1.1.0",
    "changesets-github-release": "^0.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "terser": "^5.26.0",
    "typedoc": "^0.25.4",
    "typedoc-plugin-mdn-links": "^3.1.9",
    "typescript": "^5.3.2",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.1.0",
    "webdriverio": "^8.27.0"
  }
}